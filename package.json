{
  "name": "vuebox",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.9",
    "@vueuse/core": "^8.2.6",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuetify": "3.0.0-beta.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "msw": "^0.39.2",
    "pinia-plugin-persistedstate": "^1.5.1",
    "postcss": "^8.4.12",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.8.2",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
